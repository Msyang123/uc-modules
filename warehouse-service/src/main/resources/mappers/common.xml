<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="common">
    <!-- 公共执行语句 -->
    <sql id="pager">
        <if test="rows != null and rows > 0 ">
            ORDER BY id desc
            LIMIT #{startRow}, #{rows}
        </if>
    </sql>
</mapper>